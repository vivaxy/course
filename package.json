  "homepage": "https://github.com/vivaxy/course#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@vivaxy/github-demo-scripts": "^0.2.3",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "gds && git add README.md && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,less,json,md,html,yml,yaml,pcss,jsx,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "gds": {
    "readme": "# [Course](https://vivaxy.github.io/course/index.html)\n\n# Initialize submodules\n\n`git submodule update --init`\n\n# Update submodules\n\n`git submodule foreach git pull`\n",
    "link-prefix": "https://vivaxy.github.io/course/"
  }
}
